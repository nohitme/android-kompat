# .gitignore for Android projects with settings preserved

# APK files
*.apk
*.ap_

# Dalvik VM files
*.dex

# Java class files
*.class

# Auto generated files
bin/
gen/

# Gradle files
build/
.gradle/

# Local configuration file (sdk path, etc)
local.properties

# Windows thumbnail db
Thumbs.db

# OSX files
.DS_Store

# Android Studio
.idea/
gradle.xml
*.iws
*.iml

# Keystore files
*.jks

# External native build folder generated in Android Studio 2.2 and later
.externalNativeBuild

# Google Services (e.g. APIs or Firebase)
google-services.json

# Freeline
freeline.py
freeline/
freeline_project_description.json
